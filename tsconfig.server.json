{
    "extends": "./tsconfig.json",
    "compilerOptions": {
      "outDir": "./out-tsc/server",
      "target": "ES2022",
      "module": "ESNext",
      "esModuleInterop": true,
      "moduleResolution": "node",
      "types": [
        "node"
      ]
    },
    "files": [
      "server.ts"
    ],
    "include": [
      "src/**/*.d.ts"
    ],
    "exclude": [
      "src/**/*.spec.ts",
      "src/**/*.test.ts"
    ]
  }

  